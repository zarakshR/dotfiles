// Place your key bindings in this file to override the defaultsauto[]
[
    {
        "key": "ctrl+tab",
        "command": "workbench.action.nextEditor"
    },
    {
        "key": "ctrl+shift+tab",
        "command": "workbench.action.previousEditor"
    },
    {
        "key": "ctrl+pagedown",
        "command": "-workbench.action.nextEditor"
    },
    {
        "key": "ctrl+pageup",
        "command": "-workbench.action.previousEditor"
    },
    {
        "key": "ctrl+alt+`",
        "command": "workbench.action.togglePanel"
    },
    {
        "key": "`",
        "command": "workbench.action.terminal.toggleTerminal",
        "when": "terminal.active"
    },
    {
        "key": "ctrl+numpad6",
        "command": "workbench.action.duplicateActiveEditorGroupRight"
    },
    {
        "key": "ctrl+numpad2",
        "command": "workbench.action.duplicateActiveEditorGroupDown"
    },
    {
        "key": "alt+x",
        "command": "hexEditor.openFile"
    },
    {
        "key": "shift+alt+w",
        "command": "editor.debug.action.selectionToWatch"
    },
    {
        "key": "ctrl+shift+alt+b",
        "command": "workbench.action.tasks.reRunTask"
    },
    {
        "key": "ctrl+r",
        "command": "workbench.action.tasks.runTask"
    },
    {
        "key": "alt+`",
        "command": "type",
        "args": {
            "text": "`"
        }
    },
    {
        "key": "ctrl+shift+t",
        "command": "workbench.action.reopenClosedEditor"
    },
    {
        "key": "ctrl+shift+t",
        "command": "-workbench.action.reopenClosedEditor"
    },
    {
        "key": "ctrl+shift+n",
        "command": "workbench.action.newWindow"
    },
    {
        "key": "ctrl+shift+n",
        "command": "-workbench.action.newWindow"
    },
    {
        "key": "ctrl+t s",
        "command": "workbench.view.extension.test"
    },
    {
        "key": "ctrl+m s",
        "command": "workbench.view.extension.metals-explorer"
    },
    {
        "key": "ctrl+,",
        "command": "workbench.action.openSettingsJson"
    },
    {
        "key": "ctrl+shift+,",
        "command": "-editor.action.inPlaceReplace.up",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "ctrl+shift+,",
        "command": "workbench.action.openFolderSettingsFile"
    }
]
